<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>MDOD Registratie</title>

    <!-- Style for Bootstrap -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
          integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <!-- Style -->
    <link rel="stylesheet" href="css/style.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <js src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></js>
    <js src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></js>
    <![endif]-->

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
</head>
<body>
<!-- Header added -->
<header id="header"></header>
<!--Container-->
<div class="container">
    <div class="inner_container">
        <form class="cp_form form-register">
            <h3>Wijzig uw gegevens</h3>

            <!-- Firstname inputfield -->
            <div class="form-group">
                <h4>Voornaam:</h4>
                <label for="inputVoornaam" class="sr-only">Voornaam</label>
                <input type="text" id="inputVoornaam" name="firstname" class="form-control" placeholder="Voornaam" required autofocus>
            </div>

            <!-- Infix inputfield -->
            <div class="form-group">
                <h4>Tussenvoegsel:</h4>
                <label for="inputTussenvoegsel" class="sr-only">Tussenvoegsel</label>
                <input type="text" id="inputTussenvoegsel" name="infix" class="form-control" placeholder="Tussenvoegsel">
            </div>

            <!-- Lastname inputfield -->
            <div class="form-group">
                <h4>Achternaam:</h4>
                <label for="inputAchternaam" class="sr-only">Achternaam</label>
                <input type="text" id="inputAchternaam" name="lastname" class="form-control" placeholder="Achternaam" required>
            </div>

        <!-- Phonenumber inputfield -->
        <div class="form-group">
            <h4>Telefoonnummer:</h4>
            <label for="inputTelefoonnummerRegister" class="sr-only">Telefoonnummer</label>
            <input type="text" id="inputTelefoonnummer" name="phonenumber" class="form-control" placeholder="Telefoonnummer" required>
        </div>

        <!-- Location inputfield -->
        <div class="form-group">
            <h4>Locatie:</h4>
            <label for="inputLocatieRegister" class="sr-only">Locatie</label>
            <input type="text" id="inputLocatie" name="location" class="form-control" placeholder="Locatie" required>
        </div>

            <!-- Submit inputfield -->
            <button class="btn btn-lg btn-primary btn-block" type="submit">Wijzigingen opslaan</button>
        </form>
    </div>
</div>
<!-- /Container -->
<!--Footer added-->
<footer id="footer"></footer>
</body>
    <script src="js/includehtml.js"></script>
    <script src="js/authcookie.js"></script>
    <script src="js/changeprofile.js"></script>
    <script>include();</script>
</html>