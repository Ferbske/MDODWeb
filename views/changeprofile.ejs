<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>MDOD Registratie</title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
          integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <!-- Retrieving the CSS for styling -->
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/login_register.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <js src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></js>
    <js src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></js>
    <![endif]-->

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
</head>
<body>
    <header id="header"></header>
    <div class="container" id="form">
        <form class="form-register">
            <!-- Registreren logo -->
            <!-- Register heading -->
            <h3 class="register-heading">MDOD Registratie</h3>

            <!-- Firstname inputfield -->
            <div class="form-group">
                <h4>Voornaam:</h4>
                <label for="inputVoornaam" class="sr-only">Voornaam</label>
                <input type="text" id="inputVoornaam" name="firstname" class="form-control" placeholder="Voornaam" required autofocus>
            </div>

            <!-- Infix inputfield -->
            <div class="form-group">
                <h4>Tussenvoegsel:</h4>
                <label for="inputTussenvoegsel" class="sr-only">Tussenvoegsel</label>
                <input type="text" id="inputTussenvoegsel" name="infix" class="form-control" placeholder="Tussenvoegsel">
            </div>

            <!-- Lastname inputfield -->
            <div class="form-group">
                <h4>Achternaam:</h4>
                <label for="inputAchternaam" class="sr-only">Achternaam</label>
                <input type="text" id="inputAchternaam" name="lastname" class="form-control" placeholder="Achternaam" required>
            </div>

            <!-- Phonenumber inputfield -->
            <div class="form-group">
                <h4>Telefoonnummer:</h4>
                <label for="inputTelefoonnummerRegister" class="sr-only">Telefoonnummer</label>
                <input type="text" id="inputTelefoonnummerRegister" name="phonenumber" class="form-control" placeholder="Telefoonnummer" required>
            </div>

            <!-- Location inputfield -->
            <div class="form-group">
                <h4>Locatie:</h4>
                <label for="inputLocatieRegister" class="sr-only">Locatie</label>
                <input type="text" id="inputLocatieRegister" name="location" class="form-control" placeholder="Locatie" required>
            </div>

            <!-- Submit inputfield -->
            <button class="btn btn-lg btn-primary btn-block" type="submit">Registreer</button>
        </form>
    </div> <!-- /container -->
    <br>
    <footer id="footer"></footer>
</body>
<script src="js/includehtml.js"></script>
<script>include();</script>
</html>